{
  "presets": [
    "react",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    ["babel-root-slash-import", { "rootPathSuffix": "app/javascript/src" }],
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }],
    ["transform-do-expressions"]
  ]
}
